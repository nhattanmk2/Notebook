<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomCardView">

        <attr name="state" format="enum">
            <enum name="game1" value="0" />
            <enum name="game2" value="1"/>
            <enum name="game3" value="2"/>
            <enum name="game4" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="textState" format="enum">
            <enum name="happy" value="0" />
            <enum name="sad1" value="1" />
            <enum name="sad2" value="2" />
        </attr>
    </declare-styleable>
</resources>